{
  "name": "sacred-timeline-obsidian",
  "version": "0.1.0",
  "description": "Git for humans - Obsidian plugin for version control with human-friendly language",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "git",
    "version-control",
    "sacred-timeline"
  ],
  "author": "Suhit Anantula",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.0",
    "obsidian": "latest",
    "tslib": "^2.6.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "simple-git": "^3.21.0"
  }
}
